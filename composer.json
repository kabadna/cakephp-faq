{
    "name": "kabadna/cakephp-faq",
    "description": "FAQ plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.1",
        "cakephp/migrations": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Faq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faq\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
